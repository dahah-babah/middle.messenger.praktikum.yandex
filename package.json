{
  "name": "web-messenger",
  "version": "1.0.0",
  "scripts": {
    "dev": "parcel src/index.html",
    "start": "node server.js",
    "build": "parcel build src/index.html",
    "lint": "eslint . --ext ts --ext js",
    "lint:fix": "eslint . --ext ts --ext js --fix",
    "lint:css": "stylelint \"**/*.less\""
